{
  "name": "@asap-hub/asap-cli",
  "private": true,
  "version": "0.1.0",
  "bin": "build-cjs/cli.js",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "algolia:delete-index": "ts-node --transpile-only src/cli.ts algolia:delete-index",
    "algolia:clear-index": "ts-node --transpile-only src/cli.ts algolia:clear-index",
    "algolia:get-settings": "ts-node --transpile-only src/cli.ts algolia:get-settings",
    "algolia:move-index": "ts-node --transpile-only src/cli.ts algolia:move-index",
    "algolia:process-performance": "ts-node --transpile-only src/cli.ts algolia:process-performance",
    "algolia:remove-records": "ts-node --transpile-only src/cli.ts algolia:remove-records",
    "algolia:set-settings": "ts-node --transpile-only src/cli.ts algolia:set-settings",
    "algolia:set-analytics-settings": "ts-node --transpile-only src/cli.ts algolia:set-analytics-settings",
    "build:babel": "../../scripts/build-babel.sh build",
    "watch:babel": "../../scripts/build-babel.sh watch",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@algolia/client-search": "4.22.1",
    "@asap-hub/algolia": "workspace:*",
    "@asap-hub/errors": "workspace:*",
    "@asap-hub/message-templates": "workspace:*",
    "@asap-hub/model": "workspace:*",
    "@asap-hub/services-common": "workspace:*",
    "@hapi/boom": "9.1.4",
    "@types/yargs": "17.0.5",
    "algoliasearch": "4.22.1",
    "apr-intercept": "3.0.4",
    "async-sema": "3.1.1",
    "aws-sdk": "2.1524.0",
    "csv-parse": "4.16.3",
    "got": "12.0.0",
    "jwt-encode": "1.0.1",
    "luxon": "1.28.1",
    "prettier": "3.1.1",
    "pump": "3.0.0",
    "through2-concurrent": "2.0.0",
    "uuid": "8.3.2",
    "yargs": "17.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.23.7",
    "@babel/plugin-transform-modules-commonjs": "7.23.3",
    "@babel/plugin-transform-runtime": "7.23.7",
    "@babel/runtime-corejs3": "7.23.8",
    "@types/apr-intercept": "workspace:*",
    "@types/babel__core": "7.20.5",
    "@types/hapi__boom": "9.0.1",
    "@types/jest": "29.5.12",
    "@types/jwt-decode": "2.2.1",
    "@types/jwt-encode": "workspace:*",
    "@types/luxon": "1.27.1",
    "@types/node": "20.10.5",
    "@types/prettier": "2.7.3",
    "@types/pump": "1.1.3",
    "@types/through2-concurrent": "2.0.4",
    "@types/uuid": "8.3.4",
    "@yarnpkg/pnpify": "3.1.7",
    "nock": "13.4.0"
  }
}
