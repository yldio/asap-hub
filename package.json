{
  "private": true,
  "name": "asap-hub",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "clean": "rimraf \"{apps,packages}/*/{build,*.tsbuildinfo}\" \"coverage\" \".serverless*\"",
    "build:babel": "yarn workspaces foreach -vpt run build:babel",
    "build:typecheck": "tsc -b -v $(node scripts/get-composite-ts-projects.js)",
    "build": "yarn build:typecheck && yarn build:babel && yarn workspaces foreach -vpt run typecheck && yarn workspaces foreach -vpt run build",
    "watch:babel": "yarn workspaces foreach -vpi -j 1000 run watch:babel",
    "watch:typecheck": "yarn build:typecheck -w",
    "start:backend": "yarn sls offline",
    "sls": "node serverless/link-plugins.js && yarn pnpify serverless",
    "fix:format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,html,css,scss,md,mdx,yml,yaml}\"",
    "lint:format": "prettier -l \"**/*.{js,jsx,ts,tsx,json,html,css,scss,md,mdx,yml,yaml}\"",
    "test": "jest"
  },
  "dependenciesMeta": {
    "browser-tabs-lock": {
      "built": false
    },
    "core-js": {
      "built": false
    },
    "core-js-pure": {
      "built": false
    },
    "ejs": {
      "built": false
    },
    "fsevents": {
      "built": false
    },
    "serverless": {
      "built": false
    },
    "serverless-offline": {
      "unplugged": true
    },
    "serverless-plugin-ncc": {
      "unplugged": true
    },
    "serverless-plugin-typescript": {
      "unplugged": true
    },
    "serverless-s3-sync": {
      "unplugged": true
    },
    "term-size": {
      "unplugged": false
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "maxWarnings": 0,
      "reportUnusedDisableDirectives": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime-corejs3": "^7.9.6",
    "@peculiar/webcrypto": "^1.1.1",
    "@testing-library/jest-dom": "^5.5.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "@yarnpkg/pnpify": "^2.0.0-rc.20",
    "@zeit/ncc": "^0.22.1",
    "babel-eslint": "^10.1.0",
    "babel-preset-react-app": "^9.1.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.0.0",
    "jest": "^25.4.0",
    "jest-circus": "^25.4.0",
    "jest-config": "^25.4.0",
    "jest-runner-eslint": "^0.7.6",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-typeahead": "^0.5.0",
    "param-case": "^3.0.3",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "serverless": "^1.71.1",
    "serverless-offline": "^6.1.5",
    "serverless-plugin-ncc": "^0.4.0",
    "serverless-s3-sync": "^1.12.0",
    "typescript": "~3.8.3"
  }
}
