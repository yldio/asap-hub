{
  "private": true,
  "name": "asap-hub",
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "packages/*",
    "@types/*"
  ],
  "engines": {
    "node": "16.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yldio/asap-hub.git"
  },
  "scripts": {
    "algolia:clear-index": "yarn node apps/asap-cli/build-cjs/cli.js algolia:clear-index",
    "algolia:delete-index": "TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\"}' yarn ts-node --transpile-only apps/asap-cli/src/cli.ts algolia:delete-index",
    "algolia:get-settings": "TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\"}' yarn ts-node --transpile-only apps/asap-cli/src/cli.ts algolia:get-settings",
    "algolia:move-index": "yarn node apps/asap-cli/build-cjs/cli.js algolia:move-index",
    "algolia:remove-records": "yarn node apps/asap-cli/build-cjs/cli.js algolia:remove-records",
    "algolia:set-settings": "TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\"}' yarn ts-node --transpile-only apps/asap-cli/src/cli.ts algolia:set-settings",
    "build": "yarn build:typecheck && yarn build:babel && yarn typecheck && yarn build:frontend",
    "build:babel": "yarn workspaces foreach -vp -j 2 run build:babel",
    "build:frontend": "yarn workspaces foreach -j 2 --exclude asap-hub -vp run build",
    "build:typecheck": "tsc -b -v $(node scripts/get-composite-ts-projects.js)",
    "check:packages": "yarn install --immutable --immutable-cache",
    "clean": "rimraf \"{apps,packages}/*/{build*,*.tsbuildinfo}\" \"coverage\" \".serverless*\"",
    "export:entity": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' yarn ts-node --transpile-only --project ./apps/crn-server/tsconfig.json --files=./apps/crn-server/@types ./apps/crn-server/scripts/cli.ts export",
    "fix:format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,html,css,scss,md,mdx,yml,yaml}\"",
    "lint:format": "prettier -l \"**/*.{js,jsx,ts,tsx,json,html,css,scss,md,mdx,yml,yaml}\"",
    "release:frontend": "yarn workspace @asap-hub/crn-frontend release:frontend",
    "schema:update": "./apps/crn-server/scripts/schema-update.sh",
    "start": "yarn workspaces foreach -vpi -j 1000 run start",
    "start:backend": "yarn workspace @asap-hub/crn-server start",
    "start:frontend": "yarn workspace @asap-hub/crn-frontend start",
    "start:sb": "yarn workspace @asap-hub/storybook start",
    "test": "LANG=en_US TZ=UTC jest",
    "test:browser": "yarn test --config=packages/react-components/jest-browser.config.js",
    "test:build-output": "yarn test --config=jest-build-output.config.js",
    "test:e2e": "yarn test --config=apps/e2e-tests/jest-e2e.config.js",
    "test:integration": "yarn test --config=jest-integration.config.js",
    "typecheck": "yarn workspaces foreach --exclude asap-hub -vpt run typecheck",
    "update:browser-list": "yarn up -R caniuse-lite",
    "watch:babel": "yarn workspaces foreach -vpi -j 1000 run watch:babel",
    "watch:typecheck": "yarn build:typecheck -w"
  },
  "resolutions": {
    "encoding": "0.1.13"
  },
  "dependenciesMeta": {
    "aws-sdk": {
      "built": false
    },
    "browser-tabs-lock": {
      "built": false
    },
    "core-js": {
      "built": false
    },
    "core-js-pure": {
      "built": false
    },
    "ejs": {
      "built": false
    },
    "fsevents": {
      "built": false
    },
    "term-size": {
      "unplugged": false
    },
    "thrift": {
      "unplugged": false
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "bundlewatch": {
    "files": [
      {
        "path": "apps/crn-frontend/build/**/*.js",
        "maxSize": "160 kB"
      }
    ]
  },
  "devDependencies": {
    "@asap-hub/eslint-config-asap-hub": "workspace:*",
    "@asap-hub/flags": "workspace:*",
    "@babel/cli": "7.18.6",
    "@babel/core": "7.18.6",
    "@babel/node": "7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.18.6",
    "@babel/plugin-proposal-optional-chaining": "7.18.6",
    "@babel/plugin-transform-modules-commonjs": "7.18.6",
    "@babel/plugin-transform-runtime": "7.18.6",
    "@babel/preset-env": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@babel/runtime-corejs3": "7.18.6",
    "@emotion/jest": "^11.3.0",
    "@jeysal/babel-plugin-ramda": "^2.0.1",
    "@peculiar/webcrypto": "1.4.0",
    "@testing-library/jest-dom": "5.16.4",
    "@types/jest": "26.0.24",
    "@types/webpack": "4.41.32",
    "@yarnpkg/pnpify": "3.1.3",
    "babel-eslint": "^10.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-inline-environment-variables": "0.5.0-alpha.42",
    "babel-preset-react-app": "10.0.1",
    "bundlewatch": "0.3.3",
    "dotenv": "^8.6.0",
    "escape-string-regexp": "^4.0.0",
    "eslint": "7.32.0",
    "hyperform": "^0.12.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "jest-runner-eslint": "0.11.1",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-suspend": "^1.1.2",
    "jest-watch-typeahead": "0.6.5",
    "nock": "^13.0.11",
    "node-fetch": "^3.0.0-beta.9",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "4.6.4",
    "webpack": "^5.37.0"
  },
  "dependencies": {
    "ts-node": "^10.7.0"
  },
  "packageManager": "yarn@3.2.1"
}
