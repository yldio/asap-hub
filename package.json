{
  "private": true,
  "name": "asap-hub",
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "clean": "rimraf \"{apps,packages}/*/{build,*.tsbuildinfo}\" \"coverage\" \".serverless*\"",
    "build:babel": "yarn workspaces foreach -vpt run build:babel",
    "build:typecheck": "tsc -b -v $(node scripts/get-composite-ts-projects.js)",
    "build": "yarn build:typecheck && yarn build:babel && yarn workspaces foreach -vpt run typecheck && yarn workspaces foreach -vpt run build",
    "watch:babel": "yarn workspaces foreach -vpi -j 1000 run watch:babel",
    "watch:typecheck": "yarn build:typecheck -w",
    "start": "yarn workspaces foreach -pt --interlaced run start",
    "start:backend": "docker-compose up -d && yarn sls offline",
    "sls": "node serverless/link-plugins.js && yarn pnpify serverless",
    "fix:format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,html,css,scss,md,mdx,yml,yaml}\"",
    "lint:format": "prettier -l \"**/*.{js,jsx,ts,tsx,json,html,css,scss,md,mdx,yml,yaml}\"",
    "test": "jest"
  },
  "dependenciesMeta": {
    "browser-tabs-lock": {
      "built": false
    },
    "core-js": {
      "built": false
    },
    "core-js-pure": {
      "built": false
    },
    "ejs": {
      "built": false
    },
    "fsevents": {
      "built": false
    },
    "serverless": {
      "built": false
    },
    "serverless-iam-roles-per-function": {
      "unplugged": true
    },
    "serverless-offline": {
      "unplugged": true
    },
    "serverless-plugin-typescript": {
      "unplugged": true
    },
    "serverless-s3-sync": {
      "unplugged": true
    },
    "serverless-webpack": {
      "unplugged": true
    },
    "term-size": {
      "unplugged": false
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@asap-hub/eslint-config-asap-hub": "workspace:packages/eslint-config-asap-hub",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime-corejs3": "^7.9.6",
    "@peculiar/webcrypto": "^1.1.1",
    "@testing-library/jest-dom": "^5.5.0",
    "@types/jest": "^25.2.2",
    "@yarnpkg/pnpify": "^2.0.0-rc.20",
    "babel-eslint": "^10.1.0",
    "babel-preset-react-app": "^9.1.2",
    "eslint": "^6.8.0",
    "jest": "^26.0.1",
    "jest-circus": "^26.0.1",
    "jest-emotion": "^10.0.32",
    "jest-runner-eslint": "^0.9.0",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-typeahead": "^0.6.0",
    "param-case": "^3.0.3",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "serverless": "^1.71.1",
    "serverless-iam-roles-per-function": "^2.0.2",
    "serverless-offline": "^6.1.5",
    "serverless-s3-sync": "^1.12.0",
    "serverless-webpack": "^5.3.2",
    "typescript": "~3.8.3",
    "webpack": "^5.0.0-beta.16"
  }
}
