{
  "private": true,
  "name": "asap-hub",
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "packages/*",
    "@types/*"
  ],
  "engines": {
    "node": "12.18.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yldio/asap-hub.git"
  },
  "scripts": {
    "clean": "rimraf \"{apps,packages}/*/{build*,*.tsbuildinfo}\" \"coverage\" \".serverless*\"",
    "build:babel": "yarn workspaces foreach -vp run build:babel",
    "build:typecheck": "tsc -b -v $(node scripts/get-composite-ts-projects.js)",
    "build": "yarn build:typecheck && yarn build:babel && yarn workspaces foreach -vpt run typecheck && yarn workspaces foreach -vpt run build",
    "watch:babel": "yarn workspaces foreach -vpi -j 1000 run watch:babel",
    "watch:typecheck": "yarn build:typecheck -w",
    "start": "yarn workspaces foreach -vpi -j 1000 run start",
    "start:backend": "docker-compose up -d && yarn sls offline --http-port=3333",
    "sls": "node serverless/link-plugins.js && yarn pnpify serverless",
    "fix:format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,html,css,scss,md,mdx,yml,yaml}\"",
    "lint:format": "prettier -l \"**/*.{js,jsx,ts,tsx,json,html,css,scss,md,mdx,yml,yaml}\"",
    "test:unit": "jest",
    "test:integration": "jest --config=apps/asap-server/jest-integration.config.js",
    "test:browser": "jest --config=packages/react-components/jest-browser.config.js",
    "test": "jest"
  },
  "resolutions": {
    "encoding": "0.1.13"
  },
  "dependenciesMeta": {
    "browser-tabs-lock": {
      "built": false
    },
    "core-js": {
      "built": false
    },
    "core-js-pure": {
      "built": false
    },
    "ejs": {
      "built": false
    },
    "fsevents": {
      "built": false
    },
    "serverless": {
      "built": false
    },
    "serverless-iam-roles-per-function": {
      "unplugged": true
    },
    "serverless-offline": {
      "unplugged": true
    },
    "serverless-plugin-tree-shake": {
      "unplugged": true
    },
    "serverless-s3-sync": {
      "unplugged": true
    },
    "term-size": {
      "unplugged": false
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bundlewatch": {
    "files": [
      {
        "path": "apps/frontend/build/**/*.js",
        "maxSize": "128 kB"
      }
    ]
  },
  "devDependencies": {
    "@asap-hub/eslint-config-asap-hub": "workspace:*",
    "@asap-hub/flags": "workspace:*",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime-corejs3": "^7.9.6",
    "@jeysal/babel-plugin-ramda": "^2.0.1",
    "@peculiar/webcrypto": "^1.1.1",
    "@testing-library/jest-dom": "^5.9.0",
    "@types/jest": "^26.0.3",
    "@types/webpack": "^4",
    "@yarnpkg/pnpify": "^2.2.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-inline-environment-variables": "0.5.0-alpha.42",
    "babel-preset-react-app": "^9.1.2",
    "bundlewatch": "^0.3.1",
    "codecov": "^3.8.0",
    "dotenv": "^8.2.0",
    "eslint": "^6",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.0",
    "hyperform": "^0.12.0",
    "jest": "^26.4.2",
    "jest-circus": "^26.4.2",
    "jest-emotion": "^10.0.32",
    "jest-runner-eslint": "^0.10.0",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-suspend": "^1.1.2",
    "jest-watch-typeahead": "^0.6.0",
    "nock": "^13.0.4",
    "node-fetch": "^3.0.0-beta.8",
    "param-case": "^3.0.3",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "serverless": "^1.81.1",
    "serverless-iam-roles-per-function": "^2.0.2",
    "serverless-offline": "patch:serverless-offline@^6.8.0#./patches/serverless-offline.patch",
    "serverless-plugin-tree-shake": "^1.1.2",
    "serverless-s3-sync": "^1.15.0",
    "typescript": "~3.9.7",
    "webpack": "^5.1.0"
  }
}
