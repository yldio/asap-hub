{
  "name": "@asap-hub/cms-data-sync",
  "private": true,
  "version": "0.1.0",
  "engines": {
    "node": "18.x"
  },
  "module": "build/index.js",
  "main": "build-cjs/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build:babel": "../../scripts/build-babel.sh build",
    "watch:babel": "../../scripts/build-babel.sh watch",
    "migrate": "TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\"}' ts-node ./src/index.ts",
    "schema:update:crn": "SQUIDEX_BASE_URL=$SQUIDEX_BASE_URL SQUIDEX_APP_NAME=$CRN_SQUIDEX_APP_NAME SQUIDEX_CLIENT_ID=$CRN_SQUIDEX_CLIENT_ID SQUIDEX_CLIENT_SECRET=$CRN_SQUIDEX_CLIENT_SECRET ./scripts/schema-update.sh",
    "calendar:sync:all": "ts-node ./scripts/run-calendar-sync-all.ts"
  },
  "dependencies": {
    "@asap-hub/contentful": "workspace:*",
    "@asap-hub/crn-server": "workspace:*",
    "@asap-hub/model": "workspace:*",
    "@asap-hub/server-common": "workspace:*",
    "@asap-hub/squidex": "workspace:*",
    "@babel/runtime-corejs3": "7.22.6",
    "@contentful/rich-text-types": "16.2.0",
    "cheerio": "^1.0.0-rc.12",
    "contentful-html-rich-text-converter": "yldio/contentful-html-rich-text-converter#v1.0.19",
    "contentful-management": "10.39.2",
    "graphql": "15.8.0",
    "graphql-tag": "2.12.6",
    "limiter": "^2.1.0",
    "rehype-parse": "7.0.1",
    "rehype-remark": "8.1.1",
    "rehype-sanitize": "4.0.0",
    "remark-stringify": "9.0.1",
    "unified": "9.2.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.12.1",
    "@graphql-codegen/gql-tag-operations-preset": "1.5.5",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/schema-ast": "2.5.1",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@graphql-tools/graphql-file-loader": "7.5.17",
    "@graphql-tools/load": "7.8.14",
    "@graphql-tools/mock": "8.7.20",
    "@graphql-tools/schema": "9.0.19",
    "@graphql-typed-document-node/core": "3.2.0",
    "@types/contentful-html-rich-text-converter": "workspace:^",
    "@types/jest": "29.5.3",
    "@types/jest-when": "3.5.2",
    "@types/node": "18.17.1",
    "@types/prettier": "2.7.3",
    "graphql-typescript-integration": "1.2.1",
    "jest-when": "3.5.2",
    "prettier": "2.8.8",
    "ts-node": "10.9.1"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  }
}
