{
  "name": "@asap-hub/cms-data-sync",
  "private": true,
  "version": "0.1.0",
  "engines": {
    "node": "16.x"
  },
  "module": "build/index.js",
  "main": "build-cjs/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build:babel": "../../scripts/build-babel.sh build",
    "watch:babel": "../../scripts/build-babel.sh watch",
    "migrate": "TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\"}' ts-node ./src/index.ts",
    "schema:update:crn": "SQUIDEX_BASE_URL=$SQUIDEX_BASE_URL SQUIDEX_APP_NAME=$CRN_SQUIDEX_APP_NAME SQUIDEX_CLIENT_ID=$CRN_SQUIDEX_CLIENT_ID SQUIDEX_CLIENT_SECRET=$CRN_SQUIDEX_CLIENT_SECRET ./scripts/schema-update.sh"
  },
  "dependencies": {
    "@asap-hub/model": "workspace:*",
    "@asap-hub/squidex": "workspace:*",
    "@babel/runtime-corejs3": "7.21.0",
    "@contentful/rich-text-types": "^15.13.2",
    "contentful-html-rich-text-converter": "yldio/contentful-html-rich-text-converter#v1.0.18",
    "contentful-management": "^10.19.3",
    "graphql": "15.8.0",
    "graphql-tag": "2.12.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.12.1",
    "@graphql-codegen/gql-tag-operations-preset": "1.5.5",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/schema-ast": "2.5.1",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@graphql-tools/graphql-file-loader": "7.5.16",
    "@graphql-tools/load": "7.8.13",
    "@graphql-tools/mock": "8.7.19",
    "@graphql-tools/schema": "8.5.1",
    "@graphql-typed-document-node/core": "3.1.2",
    "@types/contentful-html-rich-text-converter": "workspace:^",
    "@types/jest": "^29.4.0",
    "@types/node": "14.18.37",
    "@types/prettier": "2.7.1",
    "graphql-typescript-integration": "1.2.1",
    "prettier": "2.7.1",
    "ts-node": "10.9.1"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  }
}
