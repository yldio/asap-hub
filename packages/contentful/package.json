{
  "name": "@asap-hub/contentful",
  "private": true,
  "version": "0.1.0",
  "engines": {
    "node": "16.x"
  },
  "module": "build/index.js",
  "main": "build-cjs/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build:babel": "../../scripts/build-babel.sh build",
    "watch:babel": "../../scripts/build-babel.sh watch",
    "space:migrate:crn": "yarn ctf-migrate up --all",
    "space:migrate:crn:dryrun": "yarn ctf-migrate up --all --dry-run",
    "space:migrate:crn:count": "yarn ctf-migrate list --all | grep \\\\[pending\\\\] | wc -l",
    "space:create-environment:crn": "TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\"}' ts-node ./scripts/create-environment.ts",
    "schema:update:crn": "graphql-codegen --config codegen.yml && yarn run prettier --write ./src/crn/autogenerated-gql/*",
    "generate:migration:crn": "contentful space generate migration -s $CONTENTFUL_SPACE_ID -e $CONTENTFUL_ENV_ID"
  },
  "dependencies": {
    "@asap-hub/errors": "workspace:*",
    "@asap-hub/model": "workspace:*",
    "@babel/runtime-corejs3": "7.21.0",
    "@contentful/rich-text-html-renderer": "^15.13.1",
    "@contentful/rich-text-types": "^15.13.2",
    "@graphql-tools/graphql-file-loader": "7.5.16",
    "@graphql-tools/load": "7.8.12",
    "@graphql-tools/mock": "8.7.18",
    "contentful-management": "^10.19.3",
    "contentful-migrate": "deluan/contentful-migrate#37b9b898599b9ba48644534d3f340b4c169df212",
    "graphql": "15.8.0",
    "graphql-request": "3.4.0",
    "graphql-tag": "2.12.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.12.1",
    "@graphql-codegen/gql-tag-operations-preset": "1.5.5",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/schema-ast": "2.5.1",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@graphql-tools/schema": "8.5.1",
    "@graphql-typed-document-node/core": "3.1.2",
    "@types/node": "14.18.37",
    "contentful-cli": "1.19.1",
    "graphql-typescript-integration": "1.2.1",
    "prettier": "2.7.1",
    "ts-node": "10.9.1"
  }
}
