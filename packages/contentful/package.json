{
  "name": "@asap-hub/contentful",
  "private": true,
  "version": "0.1.0",
  "engines": {
    "node": "16.x"
  },
  "module": "build/index.js",
  "main": "build-cjs/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build:babel": "../../scripts/build-babel.sh build",
    "watch:babel": "../../scripts/build-babel.sh watch",
    "space:migrate:crn": "yarn ctf-migrate up --all",
    "space:migrate:crn:dryrun": "yarn ctf-migrate up --all --dry-run",
    "schema:update:crn": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "contentful-migrate": "0.17.0",
    "graphql-tag": "2.12.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.12.1",
    "@graphql-codegen/gql-tag-operations-preset": "1.5.5",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/schema-ast": "2.5.1",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@graphql-typed-document-node/core": "3.1.1",
    "contentful-cli": "1.15.19",
    "graphql-typescript-integration": "1.2.1"
  }
}
