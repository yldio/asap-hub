{
  "$schema": "./__json/app.json",
  "clients": {
    "default": {
      "role": "Owner",
      "name": "default"
    }
  },
  "roles": {
    "Developer": {
      "permissions": [
        "assets",
        "contents.*",
        "roles.read",
        "rules",
        "schemas",
        "workflows"
      ]
    },
    "Editor": {
      "permissions": ["assets", "contents.*", "roles.read", "workflows.read"]
    },
    "Owner": {
      "permissions": ["*"]
    },
    "Reader": {
      "permissions": ["assets.read", "contents.*.read"]
    }
  },
  "languages": {
    "en": {
      "isMaster": true,
      "isOptional": false,
      "fallback": []
    }
  },
  "contributors": {}
}
