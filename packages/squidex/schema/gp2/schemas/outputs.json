      "create": "if(new Date(ctx.data.publishDate.iv) > new Date()) {\n    reject('Publish date date cannot be greater than today')\n}\n\nif(ctx.data.workingGroups.iv.length > 0 && ctx.data.projects.iv.length > 0) {\n    reject('Can only have a Working group or Project')\n}",
      "update": "if(ctx.data.workingGroups.iv.length > 0 && ctx.data.projects.iv.length > 0) {\n    reject('Can only have a Working group or Project')\n}\n\nconst modified = Object.keys(ctx.data)\n        .filter(key => JSON.stringify(ctx.data[key]) !== JSON.stringify(ctx.oldData[key]))\n        .filter(key => key !== 'publishDate' && key !== 'adminNotes');\n        \nif (modified.length > 0) {\n    ctx.data.lastUpdatedPartial.iv = new Date().toISOString().split('.')[0]+\"Z\";\n    replace();\n}\n\n\n"
    },
